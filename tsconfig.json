{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": ["es2021"],
    "allowJs": true,
    "module": "esnext",
    "moduleResolution": "NodeNext",
    "noEmit": false,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": false,
    "strictPropertyInitialization": false,
    "target": "ESNext",
    "types": ["node"],
    "useUnknownInCatchVariables": false
  },
  "ts-node": {
    "esm": true
  }
}
